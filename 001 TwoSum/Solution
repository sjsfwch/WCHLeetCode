//我的解法，时间复杂度O(n^2)
class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int len=nums.size();
        vector<int> results;
        for(int i=0; i<len-1; i++){
            for(int j=i+1; j<len; j++){
                if(nums[i]+nums[j]==target){
                    results.push_back(i);
                    results.push_back(j);
                    return results;
                }
            }
        }
        
    }
};

//学习其他大神的解法，考虑用哈希表做
